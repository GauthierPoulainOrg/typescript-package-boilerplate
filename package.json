    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.6.2",
    "prettier": "^3.0.2",
    "rollup": "^3.28.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
