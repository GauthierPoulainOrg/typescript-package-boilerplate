  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.umd.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --watch",
